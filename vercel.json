{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/viam-web/browser"
      }
    },
    {
      "src": "dist/viam-web/server/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/viam-web/server/main.js" }
  ]
}
