{
  "name": "viam-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./node_modules/@angular/cli/bin/ng.js serve",
    "build": "node ./node_modules/@angular/cli/bin/ng.js build --configuration production",
